    public static Integer getNumero(int d, int e){
    	
    	Integer i = (int)-1;
    	
    	while (i.intValue()<d){
    		i=  (int) (1+Math.random()*e);
    	}
    	
    	return i;
    }
    
    public static Integer getDataRandomica (){
    	String  dia = "";
    	String  mes = getNumero(1, 12).toString();
    	String  ano = getNumero(1940, 1993).toString();

    	// Acrescenta numero zero quando o numero do mes for menor que 10 
    	if (Integer.parseInt(mes)<10){
    		mes = "0" + mes;
    	}
    	
    	// Trata meses com 28, 29, 30 e 31 dias 
    	if (mes=="01" || mes=="03" || mes=="05" || mes=="07" || mes=="08" || mes=="10" || mes=="12" ){
    		dia = getNumero(1, 31).toString();
    	} else if (mes=="02"){ // Tratamento para o mes de fevereiro
    		// Tratamento para ano bisexto
    		if (Integer.parseInt(ano) % 4 == 0){
    			dia = getNumero(1, 29).toString();
    		} else {
    			dia = getNumero(1, 28).toString();
    		}
    	  // Tratamento para os meses com 30 dias	
    	} else {
    		dia = getNumero(1, 30).toString();
    	}
    	
    	// Acrescenta numero zero quando o numero do dia for menor que 10 
    	if (Integer.parseInt(dia)<10){
    		dia = "0" + dia;
    	}

    	return Integer.parseInt(ano + mes +dia);
    	
    }
    
