      <component icon="pictures/tPostgresqlInput_2.png" uniqueName="tPostgresqlInput_2" label="Composicao Repasse Convenio">
        <input link="tJava_2">tJava_2</input>
        <output link="tMap_2">tMap_2</output>
        <output link="tJava_3">tJava_3</output>
        <componentType>tPostgresqlInput</componentType>
        <parameters>
          <column name="Ativar" value="true"/>
          <column name="tStatCatcher Statistics" value="false"/>
          <column name="Permitir execução em paralelo" value="false"/>
          <column name="Use an existing connection" value="false"/>
          <column name="Host" value="context.con_stg_siconv_Server"/>
          <column name="Porta" value="context.con_stg_siconv_Port"/>
          <column name="Base de dados" value="context.con_stg_siconv_Database"/>
          <column name="Schema" value="context.con_stg_siconv_Schema"/>
          <column name="Username" value="context.con_stg_siconv_Login"/>
          <column name="Senha" value="context.con_stg_siconv_Password"/>
          <column name="Nome da Tabela" value="&quot;Composicao Repasse Convenio&quot;"/>
          <column name="Guess Schema" value="&quot;&quot;"/>
          <column name="Query" value="&quot;SELECT DISTINCT pp.programa_fk as id_prog_programa,
c_atual.id as id_conv_convenio,
case when o.qualificacao_proponente = 'SEM_EMENDA_PARLAMENTAR' then '1'
     when o.qualificacao_proponente = 'BENEFICIARIO_ESPECIFICO' then '2'   
     when o.qualificacao_proponente = 'BENEFICIARIO_EMENDA_PARLAMENTAR' then '3'
else '-8' end  as id_prog_qualificacao,
siconv.valor_atual_conv_emenda_qualif(c.sequencial,c.ano,cpe.numero_emenda_parlamentar) as vl_conv_repasse_atual,
case when cpe.numero_emenda_parlamentar is null then 'NÃO SE APLICA' else cpe.numero_emenda_parlamentar end as nu_prog_emenda
FROM siconv.convenio c
INNER JOIN siconv.prop p on c.id = p.convenio_fk
INNER JOIN siconv.convenio c_atual on (((c_atual.efetivo = 't' and c_atual.id = c.id) or (c_atual.efetivo = 'f' and c_atual.id = c.ver_seguinte_fk)) and (c_atual.historico is null or c_atual.historico = 'f')) 
INNER JOIN siconv.prop p_atual on c_atual.id = p_atual.convenio_fk
INNER JOIN siconv.prop_programa pp on p.id = pp.prop_fk
INNER JOIN siconv.orgm_recurso_prop_programa o ON pp.id = o.prop_programa_fk
LEFT JOIN siconv.cnpj_programa_emenda cpe on o.cnpj_programa_emenda_fk = cpe.id
LEFT JOIN siconv.parlamentar par on substring(cpe.numero_emenda_parlamentar from 1 for 4) = par.codigo_parlamentar::text
LEFT JOIN siconv.regras r on pp.regras_fk = r.id
where (p_atual.situacao_proposta&lt;&gt;'RASCUNHO' and p_atual.situacao_proposta&lt;&gt;'PROPOSTA_CANCELADA') 
and c.efetivo = 't'  
and (o.valor_repasse notnull and o.valor_repasse &gt; 0)
&quot;"/>
          <column name="Use cursor" value="true"/>
          <column name="Tamanho do cursor" value="1000"/>
          <column name="Trim all the String/Char columns" value="false"/>
          <column name="Trim column" value="[{SCHEMA_COLUMN=id_prog_programa, TRIM=false}, {SCHEMA_COLUMN=id_conv_convenio, TRIM=false}, {SCHEMA_COLUMN=id_prog_qualificacao, TRIM=false}, {SCHEMA_COLUMN=vl_conv_repasse_atual, TRIM=false}, {SCHEMA_COLUMN=nu_prog_emenda, TRIM=false}]"/>
          <column name="Visualizar informações" value="true"/>
          <column name="Comentário" value="Componente responsável por retornar os campos de entrada  'id_prog_programa', 'id_conv_convenio', 'id_prog_qualificacao', 'vl_conv_repasse_atual' e 'nu_prog_emenda'."/>
          <column name="Use an existing validation rule" value="false"/>
        </parameters>
