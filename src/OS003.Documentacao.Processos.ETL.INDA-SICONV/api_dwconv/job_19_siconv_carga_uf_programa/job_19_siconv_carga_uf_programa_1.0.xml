case when pconv.data_fim_emenda_parlamentar is null then '01/01/1900' else 
 to_char(pconv.data_fim_emenda_parlamentar,'DD/MM/YYYY') end as dt_prog_fim_emenda_par,
case when pconv.status = 'CADASTRADO' then '1'
when pconv.status = 'DISPONIBILIZADO' then '2'
when pconv.status = 'EXCLUIDO' then '3'
when pconv.status = '' then '5' end as id_prog_situacao_programa,
