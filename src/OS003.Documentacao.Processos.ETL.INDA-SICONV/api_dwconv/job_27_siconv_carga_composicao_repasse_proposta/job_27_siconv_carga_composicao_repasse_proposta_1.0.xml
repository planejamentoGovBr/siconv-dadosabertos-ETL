          <column name="Senha" value="context.con_stg_siconv_Password"/>
          <column name="Nome da Tabela" value="&quot;Qualificacao Proposta Proponente&quot;"/>
          <column name="Guess Schema" value="&quot;&quot;"/>
          <column name="Query" value="&quot;SELECT pp.programa_fk as id_prog_programa,
pp.prop_fk as id_prop_proposta,
case when orpp.id is null then '0' else replace(cast(orpp.valor_repasse as text),'.',',') end as vl_prop_repasse,
case when cpe.numero_emenda_parlamentar is null then 'NÃO SE APLICA' else cpe.numero_emenda_parlamentar end as nu_prog_emenda
FROM siconv.prop_programa pp 
LEFT JOIN siconv.orgm_recurso_prop_programa orpp on pp.id = orpp.prop_programa_fk 
LEFT JOIN siconv.regras r on pp.regras_fk = r.id
LEFT JOIN siconv.cnpj_programa_emenda cpe on orpp.cnpj_programa_emenda_fk = cpe.id
LEFT JOIN siconv.parlamentar par on substring(cpe.numero_emenda_parlamentar FROM 1 for 4) = par.codigo_parlamentar::text
INNER JOIN siconv.prop p on pp.prop_fk = p.id
WHERE (p.historico ='f' or p.historico isnull) and (p.situacao_proposta&lt;&gt;'RASCUNHO' and p.situacao_proposta&lt;&gt;'PROPOSTA_CANCELADA')
&quot;"/>
